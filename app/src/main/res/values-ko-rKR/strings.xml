    <!-- MIUI permissions -->
    <string name="miui_permissions_title">특수 권한 필요</string>
    <string name="miui_permissions_message">%s는 최신 샤오미(MIUI) 장치에서 \'백그라운드에서 실행 중 팝업 윈도우 표시\'라는 특수 권한이 필요합니다. 수동으로 허용해야 합니다.\n\n \'바로가기\' 버튼을 누르거나 수동으로 설정을 찾아보십시오.</string>
    <string name="miui_permissions_ok_button">바로가기</string>
    <string name="miui_permissions_dont_show_again_button">다시 보지 않음</string>
