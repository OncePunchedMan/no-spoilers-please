<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.jarsilio.com.scrambledeggsif">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_scrambled_eggsif_optimized"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_scrambled_eggsif_optimized"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SettingsActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HandleImageActivity"></activity>
    </application>

</manifest>